{
    "apps": {
        "name": "koa_ts",
        "script": "bin/www.js",
        "watch": true,
        "ignore_watch": [
            "node_modules",
            "logs",
            "uploadFiles"
        ],
        "instances": "max",
        "error_file": "./logs/err.log",
        "out_file": "./logs/out.log",
        "log_date_format": "YYYY-MM-DD HH:mm:ss",
        "merge_logs": true
    }
}